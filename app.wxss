/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 100rpx 0;
  box-sizing: border-box;
} 

.page-container {
  padding: 30rpx;
}

.row-container {
  display: flex;
}

.panel-container {
  margin-bottom: 100rpx;
}

.panel-container .title {
  display: block;
  margin-bottom: 40rpx;
  text-align: center;
}

.flex-between {
  display: flex;
  justify-content: space-between;
}
button{
  border-radius: 4rpx;
}
button[type="canCommit"] {
  color:#FFFFFF; 
  background-color:#3b414d;
}
button[type="commitStyle2"] {
  color:#FFFFFF; 
  background-color:#3b414d;
}

button[disabled=""] {
  color:#fff; 
  background-color:#ccc;
}
