/* ticket */
.tickert-bg{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 999;
  background: rgba(0, 0, 0, 0.5)
}
.ticketBox{
  width: 610rpx;
  height: 907rpx;
  position: relative;
  left: 50%;
  margin-left: -305rpx;
  top: 50%;
  margin-top: -450rpx;
  z-index: 9999;
}
.ticketBox .bgImage{
  width: 610rpx;
  height: 907rpx;
}
.ticketBox .ticket-list{
  width: 464rpx;
  height: 430rpx;
  position: absolute;
  top: 270rpx;
  left: 50%;
  margin-left: -232rpx;
  overflow: scroll;
  z-index: 1001;
}
.ticketBox .ticket-list .item{
  margin-top: 10rpx;
}
.ticketBox .ticket-list .item:first-child{
  margin-top: 0;
}
.ticketBox .ticket-list .item .itemImg{
  width: 100%;
  height: 133rpx;
}
.close-btn{
  width: 65rpx;
  height: 65rpx;
  position: absolute;
  top: 50%;
  margin-top:-540rpx;
  right: 40rpx;
}
.ticketBox .item{
  position: relative;
}
.ticketBox .info{
  position: absolute;
  top: 0;
  left: 0;
  display: flex;
  color: #ef4044;
  width: 450rpx;
  height: 100%;
  box-sizing: border-box;
  padding-left: 130rpx;
  padding-top: 10rpx;
}
.ticketBox .info .symbol{
  font-size: 30rpx;
  padding-top: 50rpx;
  padding-left: 10rpx;
}
.ticketBox .info .number{
  font-size: 88rpx;
  font-weight: 600;
  width: 110rpx;
}
.ticketBox .info .infoRight{
  flex: 1;
}
.ticketBox .info .title{
  font-size: 37rpx;
  padding-top: 20rpx;
  padding-bottom: 5rpx;
}
.ticketBox .info .time{
  font-size: 16rpx;
  padding-top: 5rpx;
}
 .ticketBox .info .line{
  width: 95%; 
  height: 1rpx;
  background: #ef4044;
} 
.info1{
  padding-left: 55rpx;
  height: 100rpx;
  border-bottom: 1rpx solid #E8E9EB;
  line-height: 100rpx;
  font-size: 24rpx;
  color: #000000;
}
.info1_icon{
  position: relative;
  top: 7rpx;
  height: 32rpx;
  width: 32rpx;
  margin-right: 5rpx;
}
.info2{
  height: 113rpx;
  padding: 37rpx 55rpx;
}
.info2_title{
  font-size: 32rpx;
}
.info2_content{
  margin-top: 20rpx;
  overflow: hidden;
  font-size: 24rpx;
  color: #8C929C;
}
.btn_container{
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  bottom: 20rpx;
  padding-bottom: 40rpx;
}
.cancel {
  margin: 0 auto;
  width: 639rpx;
  height: 98rpx;
  border-radius: 4rpx;
  border: 1rpx solid #3B414D;
  font-size: 32rpx;
  line-height: 98rpx;
  color: #3B414D;
  background: #ffffff;
  text-align: center;
}
.cancel2{
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  bottom: 90rpx;
  width: 90%;
  height: 90rpx;
  line-height: 90rpx;
  border-radius: 4rpx;
  color: #ffffff;
  background-color: #3B414D;
  z-index: 9999999;
  font-size: 32rpx;
  text-align: center;
}
.map-holder{
  width: 100%;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 9999;
}
.callout2{
  position: absolute;
  top: 0;
  left: 50%;
  transform: translateX(-50%);
  font-size: 24rpx;
  color: #000000;
}
.callout2 .callout2-image{
  width: 420rpx;
  height: 95rpx;
}
.callout2-item-container{
  position: absolute;
  top: 30%;
  left: 50%;
  transform: translateX(-50%);
}
.callout-item1 {
  text-align: left;
  padding-right: 2px;
  color: #44a7ef;
  display: inline-block;
}
.callout-item2 {
  color: #000000;
  display: inline-block;
  text-align: left;
}