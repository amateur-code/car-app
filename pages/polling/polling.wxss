.info1{
  position: relative;
  padding: 20rpx 0 10rpx 36rpx;
  line-height: 15rpx;
  font-size: 13rpx;
  border-bottom: 1rpx solid #E8E9EB;
}

.info1_icon{
  position: absolute;
  top: 20rpx;
  left: 8.5rpx;
  height: 19rpx;
  width: 18rpx;
}

.info2{
  padding: 13rpx 0 13rpx;
}
.info2_title{
  font-size: 12rpx;
}

.info2_content{
  margin-top: 5rpx;
  font-size: 10rpx;
  color: #8C929C;
}

.map-holder{
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  z-index: 9999;
}

.callout2{
  transform: translate(-50%,-70rpx);
}
.callout2 .callout2-image{
  width: 177rpx;
  height: 60rpx;
}