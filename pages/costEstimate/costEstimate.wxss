.costEstimate-box{
  width: 500rpx;
  margin: 0 auto;
}
.totalFee-box{
  line-height: 40rpx;
  height: 40rpx;
  font-size: 40rpx;
  text-align: center;
}
.totalFee-box .txt{
  font-size: 11rpx;
}
.totalFee-box .txt {
  font-size: 20rpx;
}

.tip-box {
  padding: 20rpx 0 10rpx;
  color: #19191A;
  font-size: 14rpx;
  text-align: center;
}

.detail-box {
  display: flex;
  display: -webkit-flex;
  font-size: 14rpx;
  padding-bottom: 25rpx;
  align-items: center;
  justify-content: center;
}

.detail-box .left {
  max-width: 300rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
  color: #333;
}
.detail-box.isReduce, .detail-box.isReduce .left{
  color: #FF4B14;
}
.detail-box .line {
  flex: 1;
  /* padding-bottom: 25rpx; */
  margin: 0 10rpx;
  width: 100%;
  height: 0;
  border-bottom: 1px dashed #333;
}

.price-btn {
  font-size: 14rpx;
  width: 100%;
  text-align: center;
}
