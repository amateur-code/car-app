Page {
  background: #f5f5f5;
  overflow-x: hidden;
  overflow-y: auto;
}
.costEstimate-box{
  padding: 95rpx;
}
.totalFee-box{
  font-size: 100rpx;
  text-align: center;
}
.totalFee-box .txt{
  font-size: 24rpx;
}
.tip-box{
  display: flex;
  display: -webkit-flex;
  height: 35rpx;
  width: 100%;
  color: #cccccc;
  font-size: 20rpx;
  align-items: center;
  justify-content: center;
  margin: 40rpx 0;
}
.tip-box .border{
  width: 210rpx;
  height: 1rpx;
  background: #cccccc;
}
.tip-box .txt{
  color: #19191A;
  flex: 1;
  text-align: center;
}
.detail-box{
  display: flex;
  display: -webkit-flex;
  font-size: 24rpx;
  padding-bottom: 25rpx;
  align-items: center;
  justify-content: center;
}
.detail-box .left{
  color: #333333;
}
.detail-box.isReduce, .detail-box.isReduce .left{
  color: #FF4B14;
}
.detail-box .line{
  flex: 1;
  /* padding-bottom: 25rpx; */
  margin: 0 10rpx;
  width: 100%;
  height: 0;
  border-bottom: 1px dashed #333;
}
.price-btn{
  width: 100%;
  height: 30rpx;
  position: fixed;
  bottom: 80rpx;
  left: 0;
  text-align: center;
}
.price-btn text{
  font-size: 24rpx;
}
