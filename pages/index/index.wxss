/**index.wxss**/
.ws-blue {
  color: #34a9f8;
}

.ws-cost {
  color: #ff6600;
}

.tabList {
  width: 100%;
  height: 92rpx;
  font-size: 24rpx;
  color: #646566;
  display: flex;
  justify-content: center;
  align-items: center;
}

.tabList text {
  padding: 15rpx 30rpx 15rpx;
  border: 1rpx solid #fff;
  margin-left: 40rpx;
}

.tabList text.active {
  color: #000;
  border: 1rpx solid #dfdfdf;
  border-radius: 50rpx;
}
.address-wrapper {
  outline: none;
  border: none;
  box-sizing: border-box;
  padding: 0 20rpx 15rpx;
}
.address-msg {
  position: relative;
  display: block;
  width: 100%;
  height: auto;
  border: 1rpx solid #ddd;
  border-radius: 4rpx;
  background: #fff;
  position: relative;
}
