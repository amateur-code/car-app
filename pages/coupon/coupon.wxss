.header {
  font-size: 12rpx;
  width: 100%;
  text-align: center;
  color: #19191a;
  margin-bottom: 28rpx;
}
.listNull {
  font-size: 12rpx;
  text-align: center;
  margin-top: 50rpx;
  color:#999999;
}
.list{
  display: flex;
  flex-wrap: wrap;
  padding:  0 15rpx;
}
.coupon-list{
  display: flex;
  margin: 10rpx;
  justify-content: flex-start;
}
.icon-r{
  display:inline-block;
  width: 13rpx;
  height: 13rpx;
  border: 2rpx solid #C8C9CC;
  border-radius: 50%;
}
.icon{
  height: 101rpx;
  width: 40rpx;
  text-align:center;
  line-height: 101rpx;
  align-items: center;
}
.coupon-msg {
  width: 300rpx;
  height: 101rpx;
  border-radius: 5rpx;
  position: relative;
  box-shadow: 0 3rpx 5rpx #eee;
}
.coupon_bg{
  width: 300rpx;
  height: 101rpx;
}
.coupon-msg .coupon_right{
  position: absolute;
  width: 300rpx;
  height: 101rpx;
  top: 0;
  left: 0;
  display: flex;
  align-items: center;
}
.fee {
  font-size: 12rpx;
  color: #19191a;
  width: 100rpx;
  text-align: center;
}
.fee .num{
  font-size: 20rpx;
  color: #ff4b14;
}
.fee .num.otherNum{
  font-size: 20rpx;
}
.fee .num .rmbUnit{
  font-size: 15rpx;
}
.fee .unit{
  font-size: 15rpx;
  color: #ff4b14;
}
.coupon_txt{
  padding: 0 10rpx 0 20px;
  flex: 1;
}
.title {
  font-size: 13rpx;
  line-height: 16rpx;
  color: #19191a;
}
.over-time {
  font-size: 11rpx;
  color: #19191a;
}

.tip {
  margin-top: 10rpx;
  font-size: 9rpx;
  color: #9a9a9a;
  overflow:hidden; 
  text-overflow:ellipsis;
  display:-webkit-box;
  -webkit-box-orient:vertical;
  -webkit-line-clamp:3;
}



