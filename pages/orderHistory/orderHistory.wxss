.list_container{
  padding: 20rpx 0;
  display: flex;
  flex-wrap: wrap;
  width: 100%;
  background: #fcfdff;
}

.list_item{
  margin: 20rpx 8rpx;
  width: 234rpx;
  box-shadow: 0 0 14px 0 rgba(0,0,0,0.20);
  background-color: #ffffff;
}
.status_btn{
  display: flex;
  align-items: center;
  padding: 0 17rpx;
  font-size: 14rpx;
  height: 44.85rpx;
  color: #7D7E80;
  line-height: 44.85rpx;
  border-bottom: 1px solid #E8E9EB;
}
.status_btn image{
  display: inline-block;
  margin-left: 5rpx;
  width: 10rpx;
  height: 14rpx;
}
.body{
  padding: 0 17rpx 32rpx;
}

.title{
  margin-top: 24.57rpx;
  font-size: 15.6rpx;
  color: #19191A;
}

.list_item_time{
  margin-top: 16rpx;
  font-size: 14rpx;
  color: #646566;
}
.list_item_address_start{
  margin-top: 16rpx;
}
.list_item .list_item_address_start,
.list_item .list_item_address_end{
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  font-size: 14rpx;
  line-height: 15rpx;
  color: #646566;
}
.list_item .list_item_address_start image, 
.list_item .list_item_address_end image{
  width: 15rpx;
  height: 15rpx;
}


.empty{
  position: absolute;
  top: 35%;
  left: 50%;
  transform: translate(-50%, -50%);
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;

}
.empty .empty_info{
  font-size: 24rpx;
  color: #97989b;
  margin-top: 28rpx;
}
.empty .empty_img{
  width: 80rpx;
  height: 78rpx;
}