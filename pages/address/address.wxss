.page-content {
  background-color: #f5f5f5;
}
.header-wrapper {
  position: relative;
  box-sizing: border-box;
  width: 100%;
  height: 100rpx;
  background: #fff;
  position: fixed;
  line-height: 100rpx;
  left: 0;
  top: 0;
  padding: 34rpx 50rpx;
  z-index: 10;
  box-shadow:10px 0 10px 0 #eeeeee;
  text-align: left;
  overflow: hidden;
}
.cancel_btn{
  position: absolute;
  bottom: 0;
  right: 50rpx;
  font-size: 28rpx;
  color: #969799;
}

page {
  display: flex;
  height: 100%;
  background-color: #f5f5f5;
  flex-direction: column;
  color: #333;
}

.address-list-null {
  position: relative;
  box-sizing: border-box;
  margin: 100rpx 30rpx;
  font-size: 30rpx;
  text-align: center;
  color: #999;
}

.address-list-container {
  position: relative;
  box-sizing: border-box;
  margin: 100rpx 0rpx 0;
  display: block;
  height: 100%;
  background-color: #fff;
  overflow: hidden;
}

.address-list {
  max-height: 100%;
  width: 100%;
}

.address {
  position: relative;
  clear: both;
  min-height: 120rpx;
  border-bottom: 1rpx solid #eee;
  overflow: hidden;
  box-sizing: border-box;
  padding: 25rpx 50rpx 25rpx 50rpx;
  display: flex;
  justify-content: space-between;
  flex-direction: column;
}

.address:last-child {
  border-bottom: none;
}

.address-icon {
  position: absolute;
  left: 20rpx;
  width: 50rpx;
  height: 50rpx;
  margin-right: 20rpx;
}

.address-name {
  float: left;
  font-size: 28rpx;
  margin-right: 20rpx;
}

.address-area {
  float: left;
  font-size: 24rpx;
  color: #999;
}
.placeholder-class{
  vertical-align: middle;
  text-align: left;
}
.address-input{
  position: absolute;
  left: 50rpx;
  display: inline-block;
  font-size: 28rpx;
  height: 28rpx;
  vertical-align: middle;
}
.searchnone{
  position: absolute;
  width: 200rpx;
  height: 200rpx;
  top: 35%;
  left: 50%;
  transform: translateX(-50%);
  overflow: visible;
  overflow-y: visible;
}
