.page-content {
  min-height: 100vh;
  background-color: #f5f5f5;
}
.page-content.dark {
  background-color: #303240;
}

.header-wrapper {
  position: fixed;
  display: flex;
  align-items: center;
  left: 0;
  top: 0;
  width: 100%;
  height: 50rpx;
  line-height: 0;
  overflow: hidden;
  z-index: 10;
  background: #FFFFFF;
  box-shadow: 0 6px 9px 0 rgba(0,0,0,0.10);
}
.page-content.dark .header-wrapper{
  background-color: #303240;
}

.cancel_btn{
  display: flex;
  height: 27.3rpx;
  width: 75rpx;
  align-items: center;
  justify-content: center;
  font-size: 15.6rpx;
  color: #969799;
  border-right: 1px solid #C2C4C9;
}
.address-input{
  flex: 1;
  padding: 0 14.6rpx;
  font-size: 15.6rpx;
  line-height: 50rpx;
  color: #19191A;
}
.page-content.dark .address-input{
  color: #fff;
}

.address-list-container {
  position: relative;display: block;
  display: block;
  margin-top: 50rpx;
  height: 100%;
  background: #FCFDFF;
  overflow: hidden;
}
.page-content.dark  .address-list-container {
  background-color: #303240;
}

.address-list {
  max-height: 100%;
  width: 100%;
  padding: 0 19.5rpx;
}

.address {
  padding: 13.26rpx 0;
  border-bottom: 1rpx solid #eee;
}
.page-content.dark .address{
  border-bottom: 1px solid rgba(232,233,235,0.10);
}


.address-name {
  font-size: 14rpx;
  color: #19191A;
}
.page-content.dark .address-name{
  color: #fff;
}

.address-area {
  font-size: 12.48rpx;
  color: #969799;
}

.searchnone{
  position: absolute;
  left: 0; 
  top: 35%;
  width: 100%;
  font-size: 11rpx;
  color:#969799;
  text-align: center;
}
.searchnone image{
  display: block;
  width: 50rpx;
  height: 50rpx;
  margin: 0 auto 10rpx;
}
